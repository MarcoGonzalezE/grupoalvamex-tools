<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!-- Secuencias -->	
	<data noupdate="1">
		<!-- Transferencias -->
		<record id="seq_pagos_transferencias" model="ir.sequence">
			<field name="name">pagos_transferencias_sequence</field>
			<field name="code">contabilidad.pagos.transferencia</field>
			<field name="prefix">PTRANS</field>
			<field name="padding">4</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Planeacion de Produccion -->
		<record id="seq_ventas_produccion" model="ir.sequence">
            <field name="name">ventas_produccion_sequence</field>
            <field name="code">ventas.produccion</field>
            <field name="prefix">PV</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
	</data>
</odoo>